<% provide(:title, 'All users') %> 

<h1>Listing Users</h1>

<%= will_paginate %>

<ul class="users">
	<% @users.each do |user| %>
		<li>
  
	  	 <td><%= link_to user.name, user %> <% link_to user %></td>
	 	 <% if current_user.admin? && !current_user?(user) %>
	 	    | <%= link_to "edit", edit_user_path(user) %>
	    	| <%= link_to "delete", user, method: :delete, data: { confirm: "You sure?" } %>
	  	 <% end %>
	  </li>
	<% end %> 
</ul>

<%= will_paginate %>